<Window x:Class="AC_Career_Mode.FinishedRace"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AC_Career_Mode"
        mc:Ignorable="d"
        Title="FinishedRace" Height="106" Width="270">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Content="Position:"/>
        <TextBox Grid.Column="1" TextWrapping="Wrap" Text="TextBox"/>
        <Button Content="Finished" Grid.Row="1"/>
        <Button Content="Cancel" Grid.Column="1" Grid.Row="1"/>

    </Grid>
</Window>
